{
  "version": 2,
  "builds": [
    {
      "src": "/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "/initDB.js",
      "use": "@vercel/node"
    },
    {
      "src": "/database.sqlite",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.js"
    }
  ]
}
